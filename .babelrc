{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/transform-runtime",
    "transform-async-to-generator",
    "transform-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
    "transform-export-extensions"
  ]
}
